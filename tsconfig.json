{
  "compilerOptions": {
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "skipLibCheck": true,
    "outDir": "./typings/",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "declaration": true,
    "declarationMap": true,
    "baseUrl": "./",
    "paths": {
      "@/*": ["src/*"]
    },
    "lib": [
      "es2019",
      "DOM"
    ]
  },
  "include": ["src"]
}